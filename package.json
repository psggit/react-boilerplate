{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "scripts": {
    "build": "npm run webpack:ssr && npm run webpack:client",
    "start:dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "client": "nodemon --exec babel-node --presets env,stage-2 index.js",
    "start": "nodemon --exec babel-node --presets env,stage-2 index.js",
    "webpack:ssr": "webpack --config config/webpack.ssr.js  --mode development",
    "webpack:client": "webpack --config config/webpack.client.js  --mode development",
    "//start": "node index.js",
    "//webpack:ssr": "webpack --config config/webpack.ssr.js  --mode production",
    "//webpack:client": "webpack --config config/webpack.client.js  --mode development",
    "//build": "npm run webpack:ssr && npm run webpack:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psggit/react-boilerplate.git"
  },
  "keywords": [],
  "author": "Aparna Chinnaiah",
  "license": "ISC",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.4.4",
    "nodemon": "^1.18.4",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "webpack-node-externals": "^1.7.2",
    "zlib": "^1.0.5"
  },
  "homepage": "https://github.com/psggit/react-boilerplate#readme"
}
